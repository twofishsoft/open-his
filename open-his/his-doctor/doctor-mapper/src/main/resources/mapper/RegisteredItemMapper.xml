<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twofish.mapper.RegisteredItemMapper">
	<!-- 基本resultMap -->
	<resultMap type="com.twofish.domain.RegisteredItem" id="BaseResultMap">
		<id column="reg_item_id" property="regItemId"/>
		<result column="reg_item_name" property="regItemName"/>
		<result column="reg_item_fee" property="regItemFee"/>
		<result column="create_time" property="createTime"/>
		<result column="update_time" property="updateTime"/>
		<result column="create_by" property="createBy"/>
		<result column="update_by" property="updateBy"/>
		<result column="status" property="status"/>
		<result column="del_flag" property="delFlag"/>
	</resultMap>

	<sql id="Base_Query">
		SELECT
		t.reg_item_id,t.reg_item_name,t.reg_item_fee,t.create_time,t.update_time,t.create_by,t.update_by,t.status,
			t.del_flag
		FROM
		sys_registered_item t
	</sql>

</mapper>