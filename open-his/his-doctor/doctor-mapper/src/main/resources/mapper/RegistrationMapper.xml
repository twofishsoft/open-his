<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twofish.mapper.RegistrationMapper">
	<!-- 基本resultMap -->
	<resultMap type="com.twofish.domain.Registration" id="BaseResultMap">
		<id column="registration_id" property="registrationId"/>
		<result column="patient_id" property="patientId"/>
		<result column="patient_name" property="patientName"/>
		<result column="user_id" property="userId"/>
		<result column="doctor_name" property="doctorName"/>
		<result column="dept_id" property="deptId"/>
		<result column="registration_item_id" property="registrationItemId"/>
		<result column="registration_amount" property="registrationAmount"/>
		<result column="registration_number" property="registrationNumber"/>
		<result column="registration_status" property="registrationStatus"/>
		<result column="visit_date" property="visitDate"/>
		<result column="scheduling_type" property="schedulingType"/>
		<result column="subsection_type" property="subsectionType"/>
		<result column="create_time" property="createTime"/>
		<result column="update_time" property="updateTime"/>
		<result column="create_by" property="createBy"/>
	</resultMap>

	<sql id="Base_Query">
		SELECT
		t.registration_id,t.patient_id,t.patient_name,t.user_id,t.doctor_name,t.dept_id,t.registration_item_id,t.registration_amount,
			t.registration_number,t.registration_status,t.visit_date,t.scheduling_type,t.subsection_type,t.create_time,t.update_time,t.create_by
		FROM
		his_registration t
	</sql>

</mapper>