<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twofish.mapper.OrderChargeMapper">
	<!-- 基本resultMap -->
	<resultMap type="com.twofish.domain.OrderCharge" id="BaseResultMap">
		<id column="order_id" property="orderId"/>
		<result column="order_amount" property="orderAmount"/>
		<result column="ch_id" property="chId"/>
		<result column="reg_id" property="regId"/>
		<result column="patient_name" property="patientName"/>
		<result column="order_status" property="orderStatus"/>
		<result column="pay_platform_id" property="payPlatformId"/>
		<result column="pay_time" property="payTime"/>
		<result column="pay_type" property="payType"/>
		<result column="create_time" property="createTime"/>
		<result column="update_time" property="updateTime"/>
		<result column="create_by" property="createBy"/>
		<result column="update_by" property="updateBy"/>
	</resultMap>

	<sql id="Base_Query">
		SELECT
		t.order_id,t.order_amount,t.ch_id,t.reg_id,t.patient_name,t.order_status,t.pay_platform_id,t.pay_time,
			t.pay_type,t.create_time,t.update_time,t.create_by,t.update_by
		FROM
		his_order_charge t
	</sql>

</mapper>