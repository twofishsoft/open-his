<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twofish.mapper.CareOrderItemMapper">
	<!-- 基本resultMap -->
	<resultMap type="com.twofish.domain.CareOrderItem" id="BaseResultMap">
		<id column="item_id" property="itemId"/>
		<result column="co_id" property="coId"/>
		<result column="item_ref_id" property="itemRefId"/>
		<result column="item_name" property="itemName"/>
		<result column="item_type" property="itemType"/>
		<result column="num" property="num"/>
		<result column="price" property="price"/>
		<result column="amount" property="amount"/>
		<result column="remark" property="remark"/>
		<result column="status" property="status"/>
		<result column="create_time" property="createTime"/>
	</resultMap>

	<sql id="Base_Query">
		SELECT
		t.item_id,t.co_id,t.item_ref_id,t.item_name,t.item_type,t.num,t.price,t.amount,
			t.remark,t.status,t.create_time
		FROM
		his_care_order_item t
	</sql>

</mapper>