<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twofish.mapper.RoleMapper">

	<resultMap type="com.twofish.domain.Role" id="BaseResultMap">
		<result property="roleId" jdbcType="BIGINT" column="role_id"/>
		<result property="roleName" jdbcType="VARCHAR" column="role_name"/>
		<result property="roleCode" jdbcType="VARCHAR" column="role_code"/>
		<result property="roleSort" jdbcType="BIGINT" column="role_sort"/>
		<result property="remark" jdbcType="VARCHAR" column="remark"/>
		<result property="status" jdbcType="CHAR" column="status"/>
		<result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
		<result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
		<result column="create_by" jdbcType="VARCHAR" property="createBy" />
		<result column="update_by" jdbcType="VARCHAR" property="updateBy" />
	</resultMap>

</mapper>